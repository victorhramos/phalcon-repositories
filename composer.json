{
    "name": "victorhramos/phalcon-repo",
    "description": "A library which simplifies the use of the Repository pattern in Phalcon.",
    "keywords": ["phalcon", "repo", "repositories"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Ramos",
            "email": "torneiradroid@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-phalcon": "^3.2",
        "phalcon/incubator": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "MicheleAngioni\\PhalconRepositories\\": "src/PhalconRepositories"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
